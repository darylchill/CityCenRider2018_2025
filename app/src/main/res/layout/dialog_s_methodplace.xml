<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        android:text="PLACE WHERE METHOD WAS RECENTLY OBTAINED
"
        android:textAlignment="center">

    </TextView>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10sp"
            android:layout_marginEnd="10sp">

            <RadioGroup
                android:id="@+id/rdGroup_dialog_S_PlaceMethodObtained"
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <RadioButton
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="16dp"
                    android:text="Barangay Health Station (BHS)"
                    android:hint="3"
                    android:textSize="@dimen/textSize"
                    />

                <RadioButton
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Rural Health Unit (RHU)"
                    android:hint="4"
                    android:textSize="@dimen/textSize"
                    />

                <RadioButton
                    android:id="@+id/textView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Urban Health Center"
                    android:hint="5"
                    android:textSize="@dimen/textSize"
                    />

                <RadioButton
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:textSize="@dimen/textSize"
                    android:text="Lying-in Center"
                    android:hint="6"
                    />

                <RadioButton
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Municipal Hospital"
                    android:hint="7"
                    android:textSize="@dimen/textSize"
                    />

                <RadioButton
                    android:id="@+id/textView10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Private Clinic"
                    android:hint="8"
                    android:textSize="@dimen/textSize"
                    />

                <RadioButton
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Private Hospital"
                    android:hint="9"
                    android:textSize="@dimen/textSize"
                    />

                <RadioButton
                    android:id="@+id/textView12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="District Hospital"
                    android:hint="10"
                    android:textSize="@dimen/textSize"
                    />

                <RadioButton
                    android:id="@+id/textView26"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:textSize="@dimen/textSize"
                    android:text="Provincial Hospital"
                    android:hint="11"
                    />

                <RadioButton
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Regional Hospital/Public Medical Center"
                    android:hint="12"
                    android:textSize="@dimen/textSize"
                    />

                <RadioButton
                    android:id="@+id/textView15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Pharmacy"
                    android:hint="13"
                    android:textSize="@dimen/textSize"
                    />
                <RadioButton
                    android:id="@+id/textView18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="Don't know"
                    android:hint="99"
                    android:textSize="@dimen/textSize"
                    />

                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginBottom="20sp"
                    android:layout_marginTop="8dp">
                    <EditText
                        android:id="@+id/txtViewOthers_S_Others"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Others, specify"
                        android:textSize="@dimen/textSize"
                        />
                </android.support.design.widget.TextInputLayout>

            </RadioGroup>

        </LinearLayout>
    </ScrollView>

</LinearLayout>
